{
  "name": "isp-management-system",
  "version": "1.0.0",
  "description": "Comprehensive ISP Management System with WhatsApp Bot Integration",
  "main": "index.js",
  "scripts": {
    "dev": "node start-all.js dev",
    "build": "cd client && npm run build",
    "start": "node start-all.js prod",
    "setup": "chmod +x setup.sh && ./setup.sh",
    "setup:win": "setup.bat",
    "clean": "rm -rf client/.next client/out server/dist node_modules client/node_modules server/node_modules",
    "db:migrate": "cd server && npx prisma migrate dev",
    "db:seed": "cd server && npx prisma db seed",
    "db:studio": "cd server && npx prisma studio",
    "db:generate": "cd server && npx prisma generate",
    "db:push": "cd server && npx prisma db push",
    "db:reset": "cd server && npx prisma migrate reset",
    "lint": "cd client && npm run lint",
    "lint:fix": "cd client && npm run lint -- --fix",
    "test": "cd client && npm run test",
    "install:all": "npm install && cd client && npm install && cd ../server && npm install",
    "build:all": "npm run build && cd server && npm run build",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "pm2:status": "pm2 status",
    "production:all": "node start-production-all.js",
    "setup:production": "node scripts/setup-production.js",
    "setup:network": "node scripts/setup-network.js",
    "whatsapp:start": "node scripts/whatsapp-bot-integrated.js",
    "whatsapp:manage": "node scripts/manage-whatsapp.js"
  },
  "keywords": [
    "isp",
    "management",
    "whatsapp",
    "bot",
    "nextjs",
    "express",
    "prisma",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/abcdefak87/tahapdev.git"
  },
  "bugs": {
    "url": "https://github.com/abcdefak87/tahapdev/issues"
  },
  "homepage": "https://github.com/abcdefak87/tahapdev#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "pm2": "^6.0.13"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "@whiskeysockets/baileys": "^7.0.0-rc.4",
    "critters": "^0.0.23",
    "pino": "^9.11.0",
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0"
  }
}
